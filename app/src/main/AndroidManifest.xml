<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mao.com.asynchronoperation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.AsyncTaskActivity" />
        <activity android:name=".view.HandleThreadActivity" />
        <activity android:name=".view.ThreadPoolActivity" />
        <activity android:name=".view.IntentServiceActivity" />

        <service android:name=".view.utils.MyService" />
    </application>

</manifest>